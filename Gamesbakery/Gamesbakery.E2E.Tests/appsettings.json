{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "/app/logs/e2e-tests-.log",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7,
                    "shared": true
                }
            }
        ]
    },
    "E2E": {
        "BaseUrl": "http://web:80",
        "HealthCheckTimeoutSeconds": 90,
        "HealthCheckEndpoints": "health"
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=db,1433;Database=Gamesbakery;User Id=sa;Password=YourStrong@Pass;TrustServerCertificate=True;Encrypt=False;Connection Timeout=30;",
        "GuestConnection": "Server=db,1433;Database=Gamesbakery;User Id=sa;Password=YourStrong@Pass;TrustServerCertificate=True;Encrypt=False;Connection Timeout=30;",
        "AdminConnection": "Server=db,1433;Database=Gamesbakery;User Id=sa;Password=YourStrong@Pass;TrustServerCertificate=True;Encrypt=False;Connection Timeout=30;"
    },
    "TEST_DB_CONNECTION": "Server=db,1433;Database=Gamesbakery;User Id=sa;Password=YourStrong@Pass;TrustServerCertificate=True;Encrypt=False;Connection Timeout=60;"
}